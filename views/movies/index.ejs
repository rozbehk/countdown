<%- include('../partials/header') %>
<h1>Series List</h1>
<table>
  <thead>
    <tr></tr>
  </thead>
  <tbody>
    <th>poster</th>
    <th>Title</th>
    <th>Details</th>
    <% movies.forEach(function(movie){ %>
    <tr>
      <td>
        <img class="poster" src=" <%=movie.poster_path%>" alt="" />
      </td>
      <td><%= movie.title %></td>
      <td><%= movie.details %></td>
      <td>
        <a href="/movies/<%= movie._id %>">DETAILS</a>
      </td>
    </tr>

    <%})%>
  </tbody>
</table>
<%- include('../partials/footer') %>
